{
  "name": "react-duration-input-mask",
  "version": "0.1.0",
  "description": "A React component to render input masking for a duration: days, hours, minutes, seconds",
  "main": "lib/cjs/react-duration-input-mask.js",
  "module": "lib/esm/react-duration-input-mask.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nijk/react-duration-input-mask.git"
  },
  "author": "Nick Aspinall",
  "contributors": [
    "Nick Aspinall",
    "Jermaine Gyekye"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "string.prototype.matchall": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/plugin-transform-spread": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "cross-env": "^5.2.0",
    "docz": "^1.0.3",
    "docz-theme-default": "^1.0.3",
    "enzyme": "^3.4.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "husky": "^1.3.1",
    "jest": "^23.4.2",
    "prettier": "1.14.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.0-beta.8",
    "rollup-plugin-babel-minify": "^5.0.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-size-snapshot": "^0.7.0",
    "rollup-plugin-uglify": "^6.0.0",
    "styled-components": "^3.4.2"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.8"
  },
  "scripts": {
    "prebuild": "yarn run build:clean",
    "build": "yarn run build:esm && yarn run build:cjs && yarn run build:umd",
    "build:clean": "rimraf dist/ && rimraf lib/",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib/cjs",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir lib/esm",
    "build:umd": "rollup -c && rimraf dist/index.esm.js",
    "prepublish": "yarn build",
    "demo": "rollup -c rollup.config.demo.js",
    "dev": "yarn run demo -w",
    "dev:test": "jest --watch --verbose false",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "test": "jest",
    "lint": "./node_modules/prettier/bin-prettier.js --write src/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run build && git add lib/* .size-snapshot.json",
      "pre-push": "yarn test"
    }
  },
  "bugs": {
    "url": "https://github.com/nijk/react-duration-input-mask/issues"
  },
  "homepage": "https://react-duration-input-mask.netlify.com/",
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "react",
    "react-component",
    "duration",
    "input",
    "masking"
  ]
}
