{
  "name": "react-duration-input-mask",
  "version": "0.4.0",
  "description": "A React component to render input masking for a duration: days, hours, minutes, seconds",
  "main": "publish/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "/dist",
    "/publish"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nijk/react-duration-input-mask.git"
  },
  "author": "Nick Aspinall",
  "contributors": [
    "Nick Aspinall",
    "Jermaine Gyekye"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "string.prototype.matchall": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "docz": "^1.0.3",
    "docz-theme-default": "^1.0.3",
    "enzyme": "^3.4.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^23.4.2",
    "rimraf": "^2.6.3",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.3",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-size-snapshot": "^0.7.0",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "peerDependencies": {
    "prop-types": "^15.5.7",
    "react": "^16.0"
  },
  "scripts": {
    "prebuild": "rimraf dist/ && rimraf .size-snapshot.json",
    "build": "rollup -c",
    "docz:build": "docz build",
    "lint": "./node_modules/eslint/bin/eslint.js ./src",
    "prepare": "yarn build",
    "start": "docz dev",
    "test": "jest",
    "test:dev": "jest --watch --verbose false"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint --fix && git add .",
      "pre-push": "yarn test"
    }
  },
  "bugs": {
    "url": "https://github.com/nijk/react-duration-input-mask/issues"
  },
  "homepage": "https://react-duration-input-mask.netlify.com/",
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "react",
    "react-component",
    "duration",
    "input",
    "mask",
    "masking"
  ]
}
